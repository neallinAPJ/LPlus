    <link rel="shortcut icon icon" href="~/images/lexitron.png"/>
    <link rel="stylesheet" href="~/css/quirk/font-awesome.css">
    <link rel="stylesheet" href="~/css/quirk/quirk.css"> 
    <link rel="stylesheet" href="~/css/quirk/select2.css">   
